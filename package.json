{
  "name": "1-todolist",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "rs:start": "react-scripts start",
    "rs:build": "react-scripts build",
    "rs:test": "react-scripts test",
    "rs:eject": "react-scripts eject",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "NODE_ENV=test vitest",
    "coverage": "NODE_ENV=test vitest run --coverage",
    "test:ui": "NODE_ENV=test vitest --ui",
    "predeploy": "pnpm run build",
    "format": "prettier --write src",
    "lint": "eslint --fix src/**/*.{tsx,ts,jsx,js} --no-error-on-unmatched-pattern && stylelint --fix src/{,*/}*.{scss,css} --allow-empty-input"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0"
  },
  "devDependencies": {
    "@it-incubator/eslint-config": "^1.0.1",
    "@it-incubator/prettier-config": "^0.1.2",
    "@it-incubator/stylelint-config": "^0.1.5",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^18.19.6",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.1.3",
    "@vitest/ui": "^1.1.3",
    "jsdom": "^23.1.0",
    "stylelint": "^15.11.0",
    "typescript": "^4.8.4",
    "vite": "^5.0.4",
    "vite-plugin-sass-dts": "^1.3.17",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^1.1.3"
  }
}

{
  "name": "todolist",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format:write": "prettier --write \"**/*.{ts,js,cjs,md,mdx}\"",
    "format:check": "prettier --check \"**/*.{ts,js,cjs,md,mdx}\"",
    "lint:check": "eslint src --no-error-on-unmatched-pattern && stylelint src/**/*.{scss,css} --allow-empty-input",
    "lint:fix": "eslint --fix src/**/*.{tsx,ts,jsx,js} --no-error-on-unmatched-pattern && stylelint --fix src/**/*.{scss,css} --allow-empty-input",
    "test:unit": "vitest",
    "test:unit:watch": "vitest --watch",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest --coverage",
    "predeploy": "pnpm run build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@mui/icons-material": "^5.15.9",
    "@mui/material": "^5.15.9",
    "@mui/styled-engine-sc": "6.0.0-alpha.16",
    "@reduxjs/toolkit": "^2.1.0",
    "immer": "^10.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-redux": "^9.1.0",
    "styled-components": "^6.1.8",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nokken65/prettier-config": "^1.0.1",
    "@nokken65/ts-config": "^1.0.2",
    "@swc/plugin-styled-components": "^1.5.118",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.2",
    "jsdom": "^24.0.0",
    "lightningcss": "^1.23.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.1",
    "vite-plugin-sass-dts": "^1.3.17",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.0"
  }
}
